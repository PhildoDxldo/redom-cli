{
  "name": "redom-example",
  "version": "0.0.1",
  "description": "RE:DOM example project",
  "main": "server.js",
  "scripts": {
    "dev": "node watch",
    "build-css": "stylus -u nib css/index.styl -c -o public/css/main.css",
    "build-js": "rollup -c -f iife js/index.js -o public/js/main.js",
    "uglify-js": "uglifyjs public/js/main.js -cm -o public/js/main.min.js",
    "start": "node server"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "chokidar": "~1.6.1",
    "nib": "~1.1.2",
    "redom": "~2.2.3",
    "rollup": "~0.41.6",
    "rollup-plugin-buble": "~0.15.0",
    "rollup-plugin-node-resolve": "~3.0.0",
    "stylus": "~0.54.5",
    "uglify-js": "~2.8.21"
  },
  "dependencies": {
    "compression": "~1.6.2",
    "express": "~4.15.2"
  }
}
